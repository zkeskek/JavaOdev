
public class Main {

	public static void main(String[] args) {
		// for
		for (int i = 2; i <= 10; i += 2) {
			System.out.println(i);
		}
		System.out.println("For Döngü Bitti");
		
		//While
		int i=2;
		while(i<10) {
			System.out.println(i);
			i++=2;
		}
		System.out.println("For Döngü Bitti");
		
	}

}
